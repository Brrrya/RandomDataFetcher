{
  "manifest_version": 3,
  "name": "Random Data Fetcher",
  "version": "1.0",
  "description": "An extension for generating random passenger data for a website aviakassa.com",
  "action": {
    "default_popup": "hello.html",
    "default_title": "Random Data",
    "default_icon": "icon.png"
  },
  "permissions": ["tabs", "scripting", "storage", "activeTab"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
